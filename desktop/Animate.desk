                    pane panetab9 -a 0 -l 10 -H /obj/geo1/ -T SOP -m chaneditor
                    pane panetab12 -a 0 -l 0 -H /obj/ -T OBJ -m neteditor
                }
            }
            split 0.5 V {
                tabs 0 1 {
                    pane panetab8 -a 0 -l 10 -H /obj/geo1/ -T SOP -m parmeditor
                }
