            split 0.361493 V {
                tabs 0 1 {
                    pane panetab1 -a 0 -l 10 -H /obj/geo1/ -T SOP -m sceneview
                    pane panetab2 -a 0 -l 10 -H /obj/geo1/ -T SOP -m iprviewer
